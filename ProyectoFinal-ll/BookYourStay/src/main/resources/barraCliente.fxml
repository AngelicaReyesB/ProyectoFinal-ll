<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox spacing="10" style="-fx-background-color: #009900; -fx-min-height: 102.0;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.bookyourstay.controlador.BarraClienteControlador">
    <ImageView fitHeight="47.0" fitWidth="196.0" nodeOrientation="INHERIT" pickOnBounds="true" preserveRatio="true">
        <Image url="@Img/Logo.png" />
        <HBox.margin>
            <Insets left="32.0" top="28.0" />
        </HBox.margin>
    </ImageView>

    <HBox alignment="CENTER_RIGHT" prefHeight="102.0" prefWidth="1151.0" spacing="10">
        <fx:include source="barraBusquedas.fxml" />
        <Button fx:id="btnIrPerfil" minHeight="40.0" minWidth="104.0" onAction="#irPerfil" prefHeight="40.0" prefWidth="104.0" style="-fx-background-color: #006600; -fx-text-fill: white;" text="Perfil">
            <HBox.margin>
                <Insets right="10.0" />
            </HBox.margin>
        </Button>
      <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="110.0" style="-fx-background-color: #006600;" text="Cerrar Sesion" textFill="WHITE">
         <HBox.margin>
            <Insets right="24.0" />
         </HBox.margin>
      </Button>
        <HBox.margin>
            <Insets left="180.0" />
        </HBox.margin>
    </HBox>
</HBox>
